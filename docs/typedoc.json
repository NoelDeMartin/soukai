{
    "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-frontmatter"],
    "entryPointStrategy": "packages",
    "entryPoints": ["../packages/*"],
    "packageOptions": {
        "entryPoints": ["src/index.ts"],
        "compilerOptions": {
            "paths": {
                "soukai-solid": ["../soukai-solid/src"],
                "soukai-solid/*": ["../soukai-solid/src/*"],
                "soukai": ["../soukai/src"],
                "soukai/*": ["../soukai/src/*"]
            }
        }
    },
    "out": "./api",
    "entryFileName": "index",
    "hidePageHeader": true,
    "hideBreadcrumbs": true,
    "frontmatterGlobals": {
        "editLink": false,
        "search": false,
        "next": false,
        "prev": false
    }
}
