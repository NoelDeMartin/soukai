DELETE DATA {
    @prefix soukai: <https://soukai.noeldemartin.com/vocab/> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

    <#it-metadata> soukai:updatedAt "[[.*]]"^^xsd:dateTime .
} ;

INSERT DATA {
    @prefix foaf: <http://xmlns.com/foaf/0.1/> .
    @prefix soukai: <https://soukai.noeldemartin.com/vocab/> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

    <#it> foaf:member <https://berserk.fandom.com/wiki/Guts> .
    <#it-metadata> soukai:updatedAt "[[.*]]"^^xsd:dateTime .

    <#it-operation-[[.*]]>
        a soukai:Operation ;
        soukai:resource <#it> ;
        soukai:date "[[.*]]"^^xsd:dateTime ;
        soukai:property foaf:name ;
        soukai:value "Band of the Falcon" .

    <#it-operation-[[.*]]>
        a soukai:Operation ;
        soukai:resource <#it> ;
        soukai:date "[[.*]]"^^xsd:dateTime ;
        soukai:property foaf:member ;
        soukai:value
            <https://berserk.fandom.com/wiki/Griffith>,
            <https://berserk.fandom.com/wiki/Casca>,
            <https://berserk.fandom.com/wiki/Judeau>,
            <https://berserk.fandom.com/wiki/Pippin>,
            <https://berserk.fandom.com/wiki/Corkus> .

    <#it-operation-[[.*]]>
        a soukai:Operation ;
        soukai:resource <#it> ;
        soukai:type soukai:AddOperation ;
        soukai:date "[[.*]]"^^xsd:dateTime ;
        soukai:property foaf:member ;
        soukai:value <https://berserk.fandom.com/wiki/Guts> .

}
