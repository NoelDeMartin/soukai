DELETE DATA {
    @prefix foaf: <http://xmlns.com/foaf/0.1/> .
    @prefix soukai: <https://soukai.noeldemartin.com/vocab/> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

    <#nami-metadata> soukai:updatedAt "[[.*]]"^^xsd:dateTime .
} ;

INSERT DATA {
    @prefix foaf: <http://xmlns.com/foaf/0.1/> .
    @prefix soukai: <https://soukai.noeldemartin.com/vocab/> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

    <#nami> foaf:givenName "Cat Burglar" .
    <#nami-metadata> soukai:updatedAt "[[.*]]"^^xsd:dateTime .

    <#nami-operation-[[.*]]>
        a soukai:SetPropertyOperation ;
        soukai:resource <#nami> ;
        soukai:date "[[.*]]"^^xsd:dateTime ;
        soukai:property foaf:name ;
        soukai:value "Nami" .

    <#nami-operation-[[.*]]>
        a soukai:SetPropertyOperation ;
        soukai:resource <#nami> ;
        soukai:date "[[.*]]"^^xsd:dateTime ;
        soukai:property foaf:givenName ;
        soukai:value "Cat Burglar" .

}
