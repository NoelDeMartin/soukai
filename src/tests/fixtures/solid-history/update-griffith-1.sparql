DELETE DATA {
    @prefix foaf: <http://xmlns.com/foaf/0.1/> .
    @prefix soukai: <https://soukai.noeldemartin.com/vocab/> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

	<#it> foaf:name "Griffith" .
    <#it-metadata> soukai:updatedAt "[[.*]]"^^xsd:dateTime .
} ;

INSERT DATA {
    @prefix foaf: <http://xmlns.com/foaf/0.1/> .
    @prefix soukai: <https://soukai.noeldemartin.com/vocab/> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

    <#it>
        foaf:name "Femto" ;
        foaf:givenName "Wings of Darkness" .

    <#it-metadata>
        soukai:updatedAt "[[.*]]"^^xsd:dateTime .

    <#it-operation-[[.*]]>
        a soukai:SetPropertyOperation ;
        soukai:resource <#it> ;
        soukai:date "[[.*]]"^^xsd:dateTime ;
        soukai:property foaf:name ;
        soukai:value "Griffith" .

    <#it-operation-[[.*]]>
        a soukai:SetPropertyOperation ;
        soukai:resource <#it> ;
        soukai:date "[[.*]]"^^xsd:dateTime ;
        soukai:property foaf:name ;
        soukai:value "Femto" .

    <#it-operation-[[.*]]>
        a soukai:SetPropertyOperation ;
        soukai:resource <#it> ;
        soukai:date "[[.*]]"^^xsd:dateTime ;
        soukai:property foaf:givenName ;
        soukai:value "Wings of Darkness" .
}
