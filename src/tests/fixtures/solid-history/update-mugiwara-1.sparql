DELETE DATA {
    @prefix foaf: <http://xmlns.com/foaf/0.1/> .
    @prefix soukai: <https://soukai.noeldemartin.com/vocab/> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

    <#luffy-metadata> soukai:updatedAt "[[.*]]"^^xsd:dateTime .
} ;

INSERT DATA {
    @prefix foaf: <http://xmlns.com/foaf/0.1/> .
    @prefix soukai: <https://soukai.noeldemartin.com/vocab/> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

    <#luffy> foaf:givenName "Straw Hat Luffy" .
    <#luffy-metadata> soukai:updatedAt "[[.*]]"^^xsd:dateTime .

    <#luffy-operation-[[.*]]>
        a soukai:Operation ;
        soukai:resource <#luffy> ;
        soukai:date "[[.*]]"^^xsd:dateTime ;
        soukai:property foaf:name ;
        soukai:value "Monkey D. Luffy" .

    <#luffy-operation-[[.*]]>
        a soukai:Operation ;
        soukai:resource <#luffy> ;
        soukai:date "[[.*]]"^^xsd:dateTime ;
        soukai:property foaf:givenName ;
        soukai:value "Straw Hat Luffy" .

}
