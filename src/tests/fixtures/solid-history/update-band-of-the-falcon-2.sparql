DELETE DATA {
    @prefix foaf: <http://xmlns.com/foaf/0.1/> .
    @prefix soukai: <https://soukai.noeldemartin.com/vocab/> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

    <#it>
        foaf:member
            <https://berserk.fandom.com/wiki/Judeau>,
            <https://berserk.fandom.com/wiki/Pippin>,
            <https://berserk.fandom.com/wiki/Corkus> .

    <#it-metadata> soukai:updatedAt "[[.*]]"^^xsd:dateTime .

    <#griffith> foaf:name "Griffith" .
    <#griffith-metadata> soukai:updatedAt "[[.*]]"^^xsd:dateTime .
} ;

INSERT DATA {
    @prefix foaf: <http://xmlns.com/foaf/0.1/> .
    @prefix soukai: <https://soukai.noeldemartin.com/vocab/> .
    @prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

    <#it-metadata> soukai:updatedAt "[[.*]]"^^xsd:dateTime .

    <#it-operation-[[.*]]>
        a soukai:RemovePropertyOperation ;
        soukai:resource <#it> ;
        soukai:date "[[.*]]"^^xsd:dateTime ;
        soukai:property foaf:member ;
        soukai:value
            <https://berserk.fandom.com/wiki/Judeau>,
            <https://berserk.fandom.com/wiki/Pippin>,
            <https://berserk.fandom.com/wiki/Corkus> .

    <#griffith> foaf:name "Femto" .
    <#griffith-metadata> soukai:updatedAt "[[.*]]"^^xsd:dateTime .

    <#griffith-operation-[[.*]]>
        a soukai:SetPropertyOperation ;
        soukai:resource <#griffith> ;
        soukai:date "[[.*]]"^^xsd:dateTime ;
        soukai:property foaf:name ;
        soukai:value "Griffith" .

    <#griffith-operation-[[.*]]>
        a soukai:SetPropertyOperation ;
        soukai:resource <#griffith> ;
        soukai:date "[[.*]]"^^xsd:dateTime ;
        soukai:property foaf:name ;
        soukai:value "Femto" .
}
