{
  "name": "soukai",
  "version": "0.4.1",
  "description": "A refreshing library to work with your non relational database",
  "main": "dist/soukai.cjs.js",
  "module": "dist/soukai.esm.js",
  "browser": "dist/soukai.umd.js",
  "types": "dist/soukai.d.ts",
  "scripts": {
    "build": "rm dist -rf && npm run build:js && npm run build:types",
    "build:js": "noeldemartin-build-javascript",
    "build:types": "noeldemartin-build-types",
    "docs:build-typedoc": "typedoc --options docs/.typedoc/config.js ./src/index.ts",
    "docs:build-vuepress": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "docs:publish": "scripts/publish-docs.sh",
    "lint": "eslint src && tsc --noEmit",
    "publish-next": "noeldemartin-publish-next",
    "test:coverage": "jest --coverage",
    "test": "jest --verbose"
  },
  "engines": {
    "node": ">=12.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NoelDeMartin/soukai.git"
  },
  "keywords": [
    "odm"
  ],
  "author": "Noel De Martin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NoelDeMartin/soukai/issues"
  },
  "homepage": "https://soukai.js.org",
  "dependencies": {
    "@babel/runtime": "^7.12.18",
    "@noeldemartin/utils": "0.1.0-next.b86fb16b4d37341dc9b4839c50aa3d403869bfc8",
    "@types/webpack-env": "^1.13.6",
    "core-js": "^3.9.0",
    "idb": "^5.0.2"
  },
  "devDependencies": {
    "@noeldemartin/eslint-config-typescript": "0.0.0-next.bded47b9bf0a8721f5ac7317adea94f5002a10d3",
    "@noeldemartin/scripts": "0.0.0-next.fc10faa540c836a703b087e3bfe615dd68424745",
    "@strictsoftware/typedoc-plugin-monorepo": "^0.2.1",
    "@types/faker": "^4.1.2",
    "@types/jest": "^26.0.20",
    "eslint": "^7.19.0",
    "fake-indexeddb": "^3.0.0",
    "faker": "^4.1.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.1",
    "typedoc": "^0.14.2",
    "typescript": "^4.1.5",
    "vuepress": "^0.8.4"
  }
}
