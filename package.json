{
  "name": "soukai-solid",
  "version": "0.1.0",
  "description": "Solid engine for Soukai ODM",
  "main": "dist/soukai-solid.common.js",
  "module": "dist/soukai-solid.esm.js",
  "unpkg": "dist/soukai-solid.js",
  "jsdelivr": "dist/soukai-solid.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rm dist -rf && npm run build-umd && npm run build-common && npm run build-esm",
    "build-umd": "cross-env NODE_ENV=development TARGET_BUILD=umd webpack --mode='development' --no-progress --hide-modules",
    "build-common": "cross-env NODE_ENV=development TARGET_BUILD=commonjs webpack --mode='development' --no-progress --hide-modules",
    "build-esm": "cross-env NODE_ENV=development TARGET_BUILD=esmodule webpack --mode='development' --no-progress --hide-modules"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NoelDeMartin/soukai-solid.git"
  },
  "author": "Noel De Martin",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/NoelDeMartin/soukai-solid/issues"
  },
  "dependencies": {
    "rdflib": "^0.19.1",
    "solid-auth-client": "^2.2.14",
    "soukai": "^0.1.0"
  },
  "devDependencies": {
    "@types/faker": "^4.1.5",
    "@types/jest": "^23.3.13",
    "@types/rdflib": "^0.17.1",
    "cross-env": "^5.2.0",
    "events": "^3.0.0",
    "faker": "^4.1.0",
    "jest": "^24.0.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  }
}
