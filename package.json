{
    "name": "soukai",
    "version": "0.6.0",
    "type": "module",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/soukai.d.ts",
            "default": "./dist/soukai.js"
        },
        "./testing": {
            "types": "./dist/testing.d.ts",
            "default": "./dist/testing.js"
        }
    },
    "files": [
        "src",
        "dist"
    ],
    "license": "MIT",
    "author": "Noel De Martin",
    "repository": "github:NoelDeMartin/soukai",
    "scripts": {
        "build": "vite build",
