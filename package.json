    "lint": "noeldemartin-lint src",
    "publish-next": "noeldemartin-publish-next",
    "test": "jest --verbose",
    "test:summary": "jest --reporters jest-summary-reporter || exit 0",
    "test:coverage": "jest --coverage"
  },
  "engines": {
    "node": ">=12.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NoelDeMartin/soukai-solid.git"
